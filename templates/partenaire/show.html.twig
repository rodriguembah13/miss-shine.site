        <div class="card-header">
        </div>
        <div class="card-body ">

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ partenaire.logo }}</td>
            </tr>
